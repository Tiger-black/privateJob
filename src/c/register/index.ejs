<div class="J_register">
    <div class="item requestPhone">
        <input type='tel' class="phone-num" size="11" maxlength="11" placeholder="请输入手机号码"  onkeyup="this.value=this.value.replace(/\D/g,'')">
        <div class="delete"></div>
    </div>
    <div class="item requstCode">
        <input type='tel' class="identify-num" size="6" maxlength="6" placeholder="请输入验证码">
        <button type="button" class="identify <% if(data.isIdentifyDisable){ %>identifyDisable<% } %>">获取验证码</button>
    </div>
    <div class="item requestPass">
        <input type='password' size="12" maxlength="12" class="password" placeholder="请设置6-12位密码">
    </div>
    <div class="submit">
        <button type="button" class="requestCoupon disable"><%= data.buttonText %></button>
    </div>
</div>