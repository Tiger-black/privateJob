<div class="fromwarp" id="fromwarp">
	<ul>
	
		<%data.fields.forEach(function(item){ %>
			<% if(item === 'idCard'){%>
				<li class='inputItem'>
					<p class="name">身份证号</p>
					<input type="text" maxlength="18" placeholder="填写您的身份证号"  v-model="subData.idcard.lable" :class="{isErr : subData.idcard.isErr}" maxlength="18" @blur="inputBlur('idcard','iscardID','请输入正确的身份证号码')"
					@input="inputCheck('idcard','iscardID')">
				</li>
			<%} else if(item === 'phone') {%>
				<li class='inputItem'>
					<p class="name">手机号</p>
					<input type="tel" maxlength="11" placeholder="请输入您的手机号"  v-model="subData.phone.lable" :class="{isErr : subData.phone.isErr}" @blur="inputBlur('phone','isTel','请输入正确的手机号码')"
					@input="inputCheck('phone','isTel')" onkeyup="this.value=this.value.replace(/\D/g,'')">
				</li>
			<%} else if(item === 'name') {%>
				<li class='inputItem'>
					<p class="name">真实姓名</p>
					<input type="text" maxlength="30" placeholder="填写您的真实姓名" v-model="subData.name.lable" :class="{isErr : subData.name.isErr}"  @blur="inputBlur('name','isNameEZ','请输入正确的姓名')" @input="inputCheck('name','isNameEZ')">
				</li>
			<%} else if(item === 'imgCode') {%>
				<li class='inputItem'>
					<p class="name">图片验证</p>
					<input type="text" maxlength="10" placeholder="点击图片可刷新"  v-model="subData.sendImgCode.lable" :class="{isErr : subData.sendImgCode.isErr}" maxlength="6" @blur="inputBlur('sendImgCode','isCompany','请输入正确的验证码')"
					@input="inputCheck('sendImgCode','isCompany')">
					<img src="<%=data.img%>" alt="图片验证码"  @tap="sendImgCode()" class="imgCodeImg">
				</li>
			<%} else if(item === 'msgCode') {%>
				<li class='inputItem'>
					<p class="name">验证码</p>
					<input type="text" maxlength="10" placeholder="输入验证码"  v-model="subData.sendCode.lable" :class="{isErr : subData.sendCode.isErr}" maxlength="6" @blur="inputBlur('sendCode','isCompany','请输入正确的短信验证码')"
					@input="inputCheck('sendCode','isCompany')">
					<p class="msgCodeBtn" :class="{btnDisabled : !subData.phone.isOk}" @tap="sendCode()">获取验证码</p>
				</li>
			<% } %>
		<%})%>
	</ul>
	<button class="queryBtn disabled">查询</button>
</div>