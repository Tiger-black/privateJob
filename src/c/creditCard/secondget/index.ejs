<!--二次验证dialog-->
<div class="dialogMask">
    <div class="secondDialog">
        <div class="dialogTitle">
			<span class='dialog_title'>请输入申卡人手机号</span>
			<span class='dialog_close iconfont'>&#xe614;</span>
		</div>
        <div class="dialogContent">
			<div class='errorMessage'></div>		
			<div class="dialog_cont dialog_cont_phone">
			    <input type="tel" placeholder="手机号" class='dialog_phone' size="11" maxlength='11'>
			</div>
			<div class="dialog_cont" id='dialog_codenumber'>
			    <input type="tel" placeholder="请输入验证码" class='dialog_code' size="6" maxlength='6'>
			    <button class="dialog_getcode">获取验证码</button>
			</div>
        </div>
		<div class='dialog_submit'>
			<button class='second_submit'>下一步</button>
		</div>
		<div class='dialog_tips'>＊手机号需与信用卡申请者信息保持一致</div>
    </div>
</div>


